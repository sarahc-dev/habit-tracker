{
  "name": "habit-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:test": "NODE_ENV=test next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyse": "ANALYZE=true next build",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "cypress:open": "echo 'ðŸ›‘ Are you running test server?' && cypress open",
    "cypress:e2e": "cypress run --e2e",
    "cypress:component": "cypress run --component",
    "migration:generate": "drizzle-kit generate:pg",
    "migration:push": "tsx db/migrate.ts",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "npm run migration:generate && npm run migration:push",
    "db:migrate:test": "tsx --env-file=.env.test db/migrate.ts",
    "db:reset": "tsx db/reset.ts",
    "db:reset:test": "if [[ -f .env.test ]]; then tsx --env-file=.env.test db/reset.ts; else npm run db:reset; fi",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\""
  },
